{
	"name": "sm-challenge",
	"version": "0.0.1",
	"description": "Squadmakers Challenge for Backend - LLM Engineer",
	"author": "Harold Villota",
	"repository": "haroldvillota/sm-challenge",
	"main": "index.ts",
	"scripts": {
		"build": "tsc && tsup",
		"dev": "node --import=tsx --watch src/index.ts",
		"start": "node dist/index.js",
		"lint": "biome lint --fix",
		"format": "biome format --write",
		"test": "vitest run",
		"test:cov": "vitest run --coverage",
		"check": "pnpm lint && pnpm format && pnpm build && pnpm test"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "7.3.4",
		"axios": "^1.10.0",
		"cors": "2.8.5",
		"dotenv": "16.5.0",
		"express": "5.1.0",
		"express-rate-limit": "7.5.0",
		"helmet": "8.1.0",
		"http-status-codes": "2.3.0",
		"pg": "^8.16.3",
		"pg-pool": "^3.10.1",
		"pino": "9.7.0",
		"pino-http": "10.5.0",
		"swagger-ui-express": "5.0.1",
		"zod": "3.25.67"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/cors": "2.8.19",
		"@types/express": "5.0.3",
		"@types/pg": "^8.15.4",
		"@types/supertest": "6.0.3",
		"@types/swagger-ui-express": "4.1.8",
		"@vitest/coverage-v8": "3.2.3",
		"pino-pretty": "13.0.0",
		"supertest": "7.1.1",
		"tsup": "8.5.0",
		"tsx": "4.20.3",
		"typescript": "5.8.3",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.3"
	},
	"tsup": {
		"entry": ["src/index.ts"],
		"outDir": "dist",
		"format": ["esm", "cjs"],
		"target": "es2020",
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"splitting": false,
		"skipNodeModulesBundle": true
	}
}
